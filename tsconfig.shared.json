{
  "compilerOptions": {
    "composite": false,
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false,
    "target": "es5",
    "lib": ["dom", "es2015", "es2017", "es2019"],
    "strict": false,
    "downlevelIteration": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "baseUrl": "./"
  }
}
